    <h1>This is Admin Section</h1>
    
    <h2>Hello Admin! Enter Flat details!</h2>
    

   
    <form method="POST" action="/admin" enctype="multipart/form-data">

        <h4>Enter roomtype</h4>
        <select class="form-control" name="roomtype">
            <option>1 RK</option>
            <option>1 BHK</option>
            <option>2 BHK</option>
            <option>3 BHK</option>
            <option>3 BHK+</option>
        </select>

        <h4>Enter No Of Bathroom</h4>
        <select class="form-control" name="bathroom">
            <option>1 </option>
            <option>2 </option>
            <option>3</option>
            <option>3+ </option>
        </select>

        <h4>Enter No of Balcony</h4>
        <select class="form-control" name="balcony">
            <option>0</option>
            <option>1</option>
            <option>2 </option>
            <option>3</option>
            <option>3+</option>
        </select>

        <h4>Furnish Type</h4>
        <select class="form-control" name="furnish">
            <option>Fully Furnished</option>
            <option>Semi Furnished</option>
            <option>Unfurnished</option>
        </select>
        
        <h4>Parking Facilities</h4>
        <select class="form-control" name="parking">
            <option>YES</option>
            <option>NO</option>
        </select>


        <h4>Enter Status</h4>
        <input class="form-control" type="text" name="status" placeholder="Default input">


        <h4>Enter Location</h4>
        <input class="form-control" type="text" name="location" placeholder="Default input">

        <h4>Enter Address</h4>
        <input class="form-control" type="text" name="address" placeholder="Default input">

        <h4>Enter Price</h4>
        <input class="form-control" type="text" name="price" placeholder="Default input">

        <h4 for="exampleFormControlFile1">Select Photo</h4>>
        <br>
        <input type="file" class="form-control-file" name="file" id="exampleFormControlFile1">
        <br>
        
        <button type="submit" class="btn purple-gradient">submit</button>
    </form>     
